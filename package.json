{
  "name": "ihme-ui",
  "description": "Visualization tools from the Institute for Health Metrics and Evaluation",
  "version": "0.3.0",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "ui/",
    "test-utils/",
    "utils/",
    "style",
    "index.js",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "contributors": [
    {
      "name": "Gabe Medrash",
      "email": "gmedrash@gmail.com"
    },
    {
      "name": "Brett Vitaz",
      "email": "vitazbt@uw.edu"
    }
  ],
  "repository": "https://github.com/ihmeuw/ihme-ui.git",
  "bugs": "https://github.com/ihmeuw/ihme-ui/issues",
  "scripts": {
    "lint": "$(npm bin)/eslint src --ext .js,.jsx --quiet || true",
    "coverage": "BABEL_DISABLE_CACHE=1 BABEL_ENV=cli $(npm bin)/babel-node $(npm bin)/babel-istanbul cover node_modules/mocha/bin/_mocha --require ./scripts/setup-jsdom.js --report lcovonly -- `scripts/get-test-directories.sh`",
    "coverage-token": "cat ./coverage/coverage.json | ./node_modules/codecov.io/bin/codecov.io.js",
    "demo": "BABEL_ENV=webpack scripts/bundle-demos.sh",
    "test": "npm run lint && BABEL_DISABLE_CACHE=1 BABEL_ENV=cli $(npm bin)/babel-node $(npm bin)/_mocha --require ./scripts/setup-jsdom.js `./scripts/get-test-directories.sh`",
    "clean": "rm -rf test-utils ui utils style index.js",
    "transpile": "BABEL_DISABLE_CACHE=1 BABEL_ENV=cli $(npm bin)/babel --ignore test/,demo src --out-dir .",
    "combine-css": "scripts/concat-stylesheets.sh",
    "build": "npm run clean && npm run transpile && npm run combine-css",
    "travis-test": "npm run lint && npm run coverage && npm run coverage-token",
    "preversion": "npm run test",
    "version": "npm run build && git add .",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel-cli": "^6.8.0",
    "babel-eslint": "^4.1.6",
    "babel-istanbul": "^0.6.1",
    "babel-loader": "^6.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-es2015-webpack": "^6.3.14",
    "babel-preset-react": "^6.3.13",
    "block-loader": "^2.1.0",
    "chai": "^3.4.1",
    "chai-enzyme": "^0.4.0",
    "codecov.io": "^0.1.6",
    "css-loader": "^0.23.1",
    "enzyme": "^2.0.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^4.0.0",
    "eslint-plugin-react": "^4.0.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdom": "^8.1.0",
    "mocha": "^2.3.4",
    "postcss-loader": "^0.8.2",
    "react-addons-test-utils": "^15.0.2",
    "react-dom": "^15.0.1",
    "sinon": "^1.17.3",
    "style-loader": "^0.13.0",
    "webpack": "^2.1.0-beta.4"
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "classnames": "^2.2.3",
    "cuid": "^1.3.8",
    "d3-axis": "^0.3.0",
    "d3-random": "^0.2.1",
    "d3-scale": "^0.6.4",
    "d3-selection": "^0.7.0",
    "d3-shape": "^0.5.1",
    "ihme-react-select": "git+https://github.com/ihmeuw/ihme-react-select.git",
    "interact.js": "^1.2.6",
    "lodash": "^4.5.0",
    "react": "^15.0.1",
    "react-addons-shallow-compare": "^15.0.2",
    "react-faux-dom": "^2.4.0",
    "react-virtualized": "^7.0.5"
  }
}
